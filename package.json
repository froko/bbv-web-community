{
  "name": "bbv-web-community",
  "version": "1.0.0",
  "description": "bbv Web Community Monorepo",
  "author": "Patrick Ineichen",
  "repository": {
    "type": "git",
    "url": "https://github.com/froko/bbv-web-community.git"
  },
  "license": "MIT",
  "scripts": {
    "format": "prettier . --write && nx run-many -t format --parallel",
    "affected": "nx affected -t lint,build,test",
    "lint": "nx run-many -t lint --parallel",
    "build": "nx run-many -t build --parallel",
    "test": "nx run-many -t test"
  },
  "devDependencies": {
    "nx": "^19.4.2",
    "prettier": "^3.3.2"
  },
  "packageManager": "pnpm@9.7.0"
}
